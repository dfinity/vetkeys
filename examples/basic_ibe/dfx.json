{
  "canisters": {
    "basic_ibe": {
      "candid": "backend/backend.did",
      "package": "ic-vetkd-example-basic-ibe-backend",
      "type": "rust",
      "metadata": [
        {
          "name": "candid:service",
          "visibility": "public"
        }
      ]
    },
    "internet-identity": {
      "type": "pull",
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "www": {
      "dependencies": ["basic_ibe", "internet-identity"],
      "build": [
        "npm i && cd frontend && npm run build"
      ],
      "frontend": {
        "entrypoint": "frontend/dist/index.html"
      },
      "source": ["frontend/dist/"],
      "type": "assets",
      "output_env_file": ".env"
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:8000",
      "type": "ephemeral"
    }
  }
}
